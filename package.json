{
  "name": "metrics-queue",
  "version": "1.0.0",
  "description": "The Metrics Queue allows developers to react performance.marks and measures for the purpose of performance-tuning against performance goals",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "types",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest --config jestconfig.json",
    "test:coverage": "jest --config jestconfig.json --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexfigliolia/metrics-queue.git"
  },
  "keywords": [
    "performance",
    "performance API"
  ],
  "author": "Alex Figliolia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexfigliolia/metrics-queue/issues"
  },
  "homepage": "https://github.com/alexfigliolia/metrics-queue#readme",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {},
  "browserslist": [
    "defaults"
  ],
  "npmName": "metrics-queue",
  "npmFileMap": [
    {
      "basePath": "/lib/",
      "files": [
        "*.js"
      ]
    }
  ]
}
