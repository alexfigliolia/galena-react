{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint-config-airbnb-base/rules/es6",
    "plugin:jest/recommended",
    "prettier"
  ],
  "plugins": ["import", "jest", "@typescript-eslint", "prettier", "plugin:compat/recommended"],
  "rules": {
    "prettier/prettier": ["error"],
    "import/no-unresolved": "error",
    "import/no-default-export": "error",
    "no-undef": 2,
    "no-dupe-keys": 2,
    "compat/compat": 0,
    "import/named": 2,
    "import/no-cycle": 2,
    "import/no-duplicates": 2,
    "prefer-destructuring": 0,
    "class-methods-use-this": 0,
    "constructor-super": 0,
    "prefer-spread": 0
  },
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [".ts"]
    },
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true, 
        "project": "tsconfig.json"
      }
    }
  },
  "env": {
    "node": true,
    "browser": true,
    "jest/globals": true
  },
}